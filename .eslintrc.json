{
    "env": {
        "browser": true,
        "es2021": true
    },
    "parser": "babel-eslint",
    "extends": [
      "some-other-config-you-use",
      "plugin:prettier/recommended",
      "airbnb"
    ],
    
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "prettier/prettier":"error",
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    }
}
